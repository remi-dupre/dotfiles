{
    "coc.preferences.formatOnSaveFiletypes": [
        "cpp",
        "json",
        "python",
        "rust"
    ],
    "diagnostic.errorSign": "⛔",
    "diagnostic.warningSign": " ‼",
    "diagnostic.infoSign": " !",
    "diagnostic.hintSign": " ?",
    "git.addedSign.text": "│",
    "git.changedSign.text": "┊",
    "git.removedSign.text": "-",
    "git.topRemovedSign.text": "⌃",
    "git.changeRemovedSign.text": "┊-",
    "git.addedSign.hlGroup": "diffAdded",
    "git.changedSign.hlGroup": "diffChanged",
    "git.removedSign.hlGroup": "diffRemoved",
    "git.topRemovedSign.hlGroup": "diffRemoved",
    "git.changeRemovedSign.hlGroup": "diffChanged",
    "git.addGBlameToBufferVar": true,
    "json.enable": true,
    "rust-analyzer.inlayHints.chainingHints": false,
    "python.formatting.provider": "black",
    "python.linting.pylintEnabled": true,
    "pyright.disableDiagnostics": true,
    "rust-analyzer.inlayHints.maxLength": 100,
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.diagnostics.disabled": [
        "macro-error"
    ]
}
